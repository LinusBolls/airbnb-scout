{
  "manifest_version": 3,
  "version": "1.1",
  "name": "AirBnB Scout",
  "description": "View the location of AirBnB listings in Google Maps!",
  "homepage_url": "https://bolls.dev",

  "content_scripts": [
    {
      "js": ["src/index.js"],
      "matches": [
        "https://www.airbnb.com/*",
        "https://www.airbnb.de/*",
        "https://www.airbnb.co.uk/*",
        "https://www.airbnb.ca/*",
        "https://www.airbnb.fr/*",
        "https://www.airbnb.es/*",
        "https://www.airbnb.it/*",
        "https://www.airbnb.jp/*",
        "https://www.airbnb.com.au/*",
        "https://www.airbnb.cn/*",
        "https://www.airbnb.ru/*",
        "https://www.airbnb.in/*",
        "https://www.airbnb.mx/*",
        "https://www.airbnb.com.br/*",
        "https://www.airbnb.ie/*",
        "https://www.airbnb.nl/*",
        "https://www.airbnb.dk/*",
        "https://www.airbnb.fi/*",
        "https://www.airbnb.se/*",
        "https://www.airbnb.no/*",
        "https://pingpongmap.net/*"
      ]
    }
  ],
  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  }
}
