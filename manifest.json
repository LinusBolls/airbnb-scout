{
  "manifest_version": 3,
  "version": "1.0",
  "name": "Octoplay",
  "description": "Play music inside of octopi!",
  "homepage_url": "https://bolls.dev",

  "content_scripts": [
    {
      "js": ["dist/index.js"],
      "matches": ["http://codeoctopi.local/*"]
    }
  ],
  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  }
}
